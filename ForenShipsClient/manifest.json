{
	"manifest_version": 2,
	"name": "ForenShips",
	"version": "1.0.2",
	"description": "Client for ForenShips, a debugging tool for friendships and relationships",
	"key": "",
	"page_action": {
		"default_title": "ForenShips",
		"default_popup": "display/display.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"permissions": [
	  "activeTab",
	  "cookies",
	  "webRequest",
	  "*://*.facebook.com/*",
	  "http://localhost:8000/*",
	  "http://forenships.co/*",
	  "declarativeContent"
	]
}
